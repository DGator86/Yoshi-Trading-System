{
  "agents": {
    "defaults": {
      "workspace": "~/clawd",
      "model": {
        "primary": "google/gemini-3-flash-preview",
        "fallbacks": ["google/gemini-2.5-flash", "openai/gpt-4o-mini"]
      },
      "thinkingDefault": "low"
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "identity": {
          "name": "ClawdBot",
          "theme": "crypto trading assistant",
          "emoji": "\ud83e\udd16"
        }
      }
    ]
  },
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_TOKEN}"
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "botToken": "${TELEGRAM_BOT_TOKEN}",
      "dmPolicy": "open",
      "allowFrom": ["*"]
    }
  },
  "skills": {
    "load": {
      "extraDirs": ["/root/ClawdBot-V1/skills"]
    }
  }
}
